{
    "name": "justijndepover/laravel-google-recaptcha-v2",
    "description": "Easily add Google Recaptcha V2 to your Laravel application forms",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justijn Depover",
            "email": "justijndepover@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "Justijndepover\\GoogleRecaptchaV2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Justijndepover\\GoogleRecaptchaV2\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Justijndepover\\GoogleRecaptchaV2\\GoogleRecaptchaV2ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
